{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowAccessFromZscaler",
            "Effect": "Allow",
            "Action": [
                "workspaces:*"
            ],
            "Resource": "*",
            "Condition": {
                "IpAddress": {
                    "aws:SourceIp": ${jsonencode(zscaler_ips)}
                }
            }
        }
    ]
}
